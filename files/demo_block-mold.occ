block_cylinder_shape =i. read_step_file('files\cylinder_block.stp')
display.DisplayShape(block_cylinder_shape, update=True)
box = i.BRepPrimAPI_MakeBox(30, 60, 90).Shape()
trns = i.gp_Trsf()
trns.SetTranslation(i.gp_Vec(0, -60, -35))
mold_basis = i.BRepBuilderAPI_Transform(box, trns).Shape()
display.DisplayShape(mold_basis, update=True, material=i.Graphic3d_NOM_STEEL)
mold = i.BRepAlgoAPI_Cut(mold_basis, block_cylinder_shape).Shape()
display.EraseAll()
display.DisplayShape(mold, update=True, material=i.Graphic3d_NOM_STEEL)